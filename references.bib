@article{chen_multiphysics_2011,
	series = {22nd {International} {Conference} on {Parallel} {Computational} {Fluid} {Dynamics} ({ParCFD} 2010){ParCFD}},
	title = {Multiphysics simulations of rocket engine combustion},
	volume = {45},
	issn = {0045-7930},
