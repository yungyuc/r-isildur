@article{shankar_three-dimensional_1989,
	title = {A three-dimensional {Maxwell}'s equation solver for computation of scattering from layered media},
	volume = {25},
	issn = {0018-9464},
	doi = {10.1109/20.34380},
	abstract = {The differential time-domain Maxwell's equations are cast in a
conservative form and then solved using a finite-volume discretization
procedure derived from computational fluid dynamics methods applied to
linear/nonlinear gasdynamics equations. The formulation accounts for any
variations in the material properties (time, space, and frequency
dependence) and can handle thin resistive sheets and lossy coatings by
positioning them at finite-volume cell boundaries. The time-domain
approach handles both continuous-wave (single-frequency) and pulsed
