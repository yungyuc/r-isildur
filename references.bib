@article{smit_geometry_1995,
	title = {The geometry of the hyperbolic system for an anisotropic perfectly elastic medium},
	volume = {167},
	url = {http://dx.doi.org/10.1007/BF02100588},
	doi = {10.1007/BF02100588},
	abstract = {We evaluate the fundamental solution of the hyperbolic system describing the generation and propagation of elastic waves in an anisotropic solid by studying the homology of the algebraic hypersurface defined by the characteristic equation, also known as the “slowness” surface. Our starting point is the Herglotz-Petrovsky-Leray integral representation of the fundamental solution. We find an explicit decomposition of the latter solution into integrals over vanishing cycles associated with the isolated singularities on the slowness surface. As is well known in the theory of isolated singularities, integrals over vanishing cycles satisfy a system of differential equations known as Picard-Fuchs equations. Such equations are linear and can have at most regular singular points. We discuss a method to obtain these equations explicitly. Subsequently, we use the monodromy properties around the regular singular points to find the asymptotic behavior according to the different types of singularities that may appear on a wave front in three dimensions. This is a method alternative to the one that arises in the Maslov theory of oscillating integrals. Our work sheds new light on how to compute and classify the Cagniard-De Hoop contour in the complex radial horizontal slowness plane; this contour is used in numerical integration schemes to obtain the full time behaviour of the fundamental solution for a given direction of propagation.},
	number = {2},
	urldate = {2009-04-06},
	journal = {Communications in Mathematical Physics},
	author = {Smit, Dirk-J. and Hoop, Maarten},
	month = feb,
	year = {1995},
	pages = {255--300},
}

