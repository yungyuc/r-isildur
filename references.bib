@inproceedings{pao_electromagnetic_1978,
	title = {Electromagnetic forces in deformable continua},
	volume = {4},
	url = {http://adsabs.harvard.edu/abs/1978meto....4..209P},
	abstract = {The main objective in the reported investigation is related to the determination of the interaction of the stress-strain field with the electromagnetic field in deformable bodies. The electromagnetic fields exert forces and torques on the induced sources in the matter and cause the latter to move and deform. Such interactions are considered for materials which can be polarized and magnetized, and which are thermally and electrically conductive, and are mechanically deformable. Balance equations of continuum mechanics are considered along with Maxwell equations for media at rest and for moving media, the Maxwell stress tensor and Minkowski energy-momentum tensor, total energy-momentum tensors, the theory of electrons and statistical mechanics, macroscopic Maxwell-Lorentz forces, magnetostatic forces on a whole body, and models for field-matter interaction.},
