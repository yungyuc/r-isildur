	author = {Mayer, M. and Zaglmayr, S. and Wagner, K. and Schoberl, J.},
	year = {2007},
	keywords = {Al, BEM, LiTaO3, absorbing boundary conditions, acoustic devices, acoustic field, acoustic materials, acoustic wave absorption, acoustic wave reflection, aluminium, aluminum electrodes, electrodes, finite element analysis, harmonic admittances, lithium compounds, microacoustic devices, micromechanical devices, parasitic acoustic wave radiation, perfectly matched layer finite element simulation, piezoelectric devices, piezoelectric equations, piezoelectric half space problem, piezoelectric materials, reflection suppression, wave field visualizations},
