@article{zhang_solving_2006,
	title = {Solving the {MHD} equations by the space-time conservation element and solution element method},
	volume = {214},
	url = {http://dx.doi.org/10.1016/j.jcp.2005.10.006},
	abstract = {We apply the space-time conservation element and solution element (CESE) method to solve the ideal MHD equations with special emphasis on satisfying the divergence free constraint of magnetic field, i.e., [backward difference] [middle dot] B = 0. In the setting of the CESE method, four approaches are employed: (i) the original CESE method without any additional treatment, (ii) a simple corrector procedure to update the spatial derivatives of magnetic field B after each time marching step to enforce [backward difference] [middle dot] B = 0 at all mesh nodes, (iii) a constraint-transport method by using a special staggered mesh to calculate magnetic field B, and (iv) the projection method by solving a Poisson solver after each time marching step. To demonstrate the capabilities of these methods, two benchmark MHD flows are calculated: (i) a rotated one-dimensional MHD shock tube problem and (ii) a MHD vortex problem. The results show no differences between different approaches and all results compare favorably with previously reported data.},
	number = {2},
	urldate = {2009-03-31},
	journal = {Journal of Computational Physics},
	author = {Zhang, Moujin and Yu, Sheng-Tao John and Lin, S.-C. Henry and Chang, Sin-Chung and Blankson, Isaiah},
	month = may,
	year = {2006},
	keywords = {MHD, cese, cfd, multi-dimension},
	pages = {599--617},
}

